#Checking for non-uniformed data in film table

SELECT DISTINCT 
 film_id,
 title,
 release_year,
 language_id,
 rental_duration,
 rental_rate,
 length,
 replacement_cost,
 rating,
 last_update,
 special_features,
 fulltext
FROM film

#Checking for non-uniformed data in customer table

SELECT DISTINCT
 customer_id,
 store_id,
 first_name,
 email,
 address_id,
 activebool,
 create_date,
 last_update, 
 active
FROM customer
